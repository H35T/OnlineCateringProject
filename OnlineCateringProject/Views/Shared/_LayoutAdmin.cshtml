    <!-- preloader area start -->
    <div id="preloader">
        <div class="loader"></div>
    </div>
    <!-- preloader area end -->
    <!-- page container area start -->
    <div class="page-container">
        <!-- sidebar menu area start -->
        <div class="sidebar-menu">
            <div class="sidebar-header">
                <div class="logo">
                    <a href="index.html"><img src="~/admin/assets/images/icon/logo.png" alt="logo"></a>
                </div>
            </div>
            <div class="main-menu">
                <div class="menu-inner">
                    <nav>
                        <ul class="metismenu" id="menu">
                            <li class="@(ViewContext?.RouteData?.Values?["controller"]?.ToString() == "HomeAdmin" ? "active" : "")">
                                <a asp-area="Admin" asp-controller="HomeAdmin" asp-action="Index" aria-expanded="true"><i class="ti-dashboard"></i><span>dashboard</span></a>

                            </li>

                            <li class="@(ViewContext?.RouteData?.Values?["controller"]?.ToString() == "Customers" ? "active" : "")">
                                <a asp-area="Admin" asp-controller="Customers" asp-action="Index" aria-expanded="true">
                                    <i class="fa fa-table"></i>
                                    <span>Customers</span>
                                </a>

                            </li>
                            <li class="@(ViewContext?.RouteData?.Values?["controller"]?.ToString() == "Caterers" ? "active" : "")">

                                <a asp-area="Admin" asp-controller="Caterers" asp-action="Index" aria-expanded="true">
                                    <i class="fa fa-table"></i>
                                    <span>Caterers</span>
                                </a>

                            </li>
                            <li class="@(ViewContext?.RouteData?.Values?["controller"]?.ToString() == "MenuCategories" ? "active" : "")">

                                <a asp-area="Admin" asp-controller="MenuCategories" asp-action="Index" aria-expanded="true">
                                    <i class="fa fa-table"></i>
                                    <span>MenuCategories</span>
                                </a>

                            </li>
                            <li class="@(ViewContext?.RouteData?.Values?["controller"]?.ToString() == "Menus" ? "active" : "")">

                                <a asp-area="Admin" asp-controller="Menus" asp-action="Index" aria-expanded="true">
                                    <i class="fa fa-table"></i>
                                    <span>Menus</span>
                                </a>

                            </li>
                            <li class="@(ViewContext?.RouteData?.Values?["controller"]?.ToString() == "CustomerInvoices" ? "active" : "")">

                                <a asp-area="Admin" asp-controller="CustomerInvoices" asp-action="Index" aria-expanded="true">
                                    <i class="fa fa-table"></i>
                                    <span>CustomerInvoices</span>
                                </a>

                            </li>
                        </ul>
                    </nav>
                </div>
            </div>
        </div>
        <!-- sidebar menu area end -->
        <!-- main content area start -->
        <div class="main-content-inner">
            <div class="row">
                <!-- data table start -->
                <div class="col-12 mt-5">
                    <div class="card">
                        <div class="card-body">
                            <div class="data-tables">
                                <!-- page title area start -->
                                <div class="page-title-area">
                                    <div class="row align-items-center">
                                        <div class="col-sm-6">
                                          @*  <div class="breadcrumbs-area clearfix">
                                                <h4 class="page-title pull-left">@ViewBag</h4>
                                            </div>*@
                                        </div>
                                        @{
                                      var userName = Context.Session.GetString("access_admin");

                                        }
                                        <div class="col-sm-6 clearfix">
                                            <div class="user-profile pull-right">
                                                <img class="avatar user-thumb" src="~/admin/assets/images/author/avatar.png" alt="avatar">
                                                <h4 class="user-name dropdown-toggle" data-toggle="dropdown">@userName<i class="fa fa-angle-down"></i></h4>
                                                <div class="dropdown-menu">
                                                    <a class="dropdown-item" href="#">Message</a>
                                                    <a class="dropdown-item" href="#">Settings</a>
                                                    <a class="dropdown-item" href="/admin/logout">Log Out</a>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!-- page title area end -->
                                @RenderBody()

                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div>

        <!-- main content area end -->
    </div>
