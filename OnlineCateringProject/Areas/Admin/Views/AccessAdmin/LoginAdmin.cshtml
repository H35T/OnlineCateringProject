@model OnlineCateringProject.Models.LoginMaster
@{
    ViewData["Title"] = "LoginAdmin";
    Layout = "~/Views/Shared/_LayoutLoginAdmin.cshtml";
}
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
<form method="post" action="@Url.Action("LoginAdmin","AccessAdmin")">
    <div class="login-form-head">
        <h4>Sign In</h4>
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>

    </div>
    <div class="login-form-body">
        <div class="form-gp">
            <label for="exampleInputEmail1">Email address</label>
            <input autofocus type="text" asp-for="Name" id="exampleInputEmail1">
            <i class="ti-email"></i>
            @*<div class="text-danger"><span asp-validation-for="Name"></span></div>*@
        </div>
        <div class="form-gp">
            <label for="exampleInputPassword1">Password</label>
            <input type="password" asp-for="Password" id="exampleInputPassword1">
            <i class="ti-lock"></i>
            @*<div class="text-danger"><span asp-validation-for="Password"></span></div>*@
        </div>
        <div class="submit-btn-area">
            <button id="form_submit" type="submit">Submit <i class="ti-arrow-right"></i></button>
        </div>
    </div>
</form>